{
    "app_name": "Depsland",
    "app_version": "0.1.0-test.6",
    "description": "Python dependencies manager specific for pyportable-installer.",
    "authors": ["Likianta <likianta@foxmail.com>"],
    "build": {
        "proj_dir": "../depsland",
        "dist_dir": "../dist/depsland-v{app_version}-win64-standard",
        "launcher_name": "setup",
        "icon": "",
        "target": [
            {
                "file": "../depsland/setup/main.py",
                "function": "main",
                "args": ["python38"],
                "kwargs": {}
            }
        ],
        "readme": "",
        "attachments": {
            "../build/assets/windows": "assets",
            "../build/depsland.bat": "asset",
            "../conf": "assets",
            "../pypi/cache": "only_folder",
            "../pypi/downloads": "only_folder",
            "../pypi/extracted": "only_folder",
            "../pypi/index": "only_folder",
            "../requirements.txt": "asset,dist:build/{name}",
            "../sidework": "assets,compile",
            "../venv_home/inventory": "only_folder",
            "../venv_home/venv_links": "only_folder"
        },
        "attachments_exclusions": [],
        "attachments_exist_scheme": "override",
        "module_paths": [],
        "module_paths_scheme": "translate",
        "venv": {
            "enable_venv": true,
            "python_version": "3.8",
            "mode": "embed_python",
            "options": {
                "depsland": {
                    "venv_name": "{app_name_lower}_venv",
                    "venv_id": "",
                    "requirements": "../requirements.txt",
                    "offline": false,
                    "local": ""
                },
                "source_venv": {
                    "path": "../venv1",
                    "copy_venv": true
                },
                "pip": {
                    "requirements": [],
                    "pypi_url": "https://pypi.python.org/simple/",
                    "local": "",
                    "offline": false
                },
                "embed_python": {
                    "path": "../.venv/python38/embed_venv"
                }
            }
        },
        "compiler": {
            "name": "pyportable_crypto",
            "options": {
                "cythonize": {
                    "c_compiler": "msvc",
                    "python_path": "_auto_detect"
                },
                "pyarmor": {
                    "liscense": "trial",
                    "obfuscate_level": 0
                },
                "pyc": {
                    "optimize_level": 0
                },
                "pyportable_crypto": {
                    "key": "we1c0me_to_depsland",
                    "c_compiler": "msvc",
                    "python_path": "E:\\programs\\python\\Python38\\python.exe"
                },
                "zipapp": {
                    "password": ""
                }
            }
        },
        "experimental_features": {
            "add_pywin32_support": false
        },
        "enable_console": true
    },
    "note": [
        "notice: depsland cannot package itself based on `venv:mode:depsland`",
        "FIXME: you need to manually copy '~/dist/lib/*' to '~/dist/venv/lib/site-packages/*'"
    ],
    "pyportable_installer_version": "4.0.0b2"
}
