{
    "app_name": "Depsland",
    "app_version": "0.2.0-test.2",
    "description": "Python dependencies manager specific for pyportable-installer.",
    "author": "likianta<likianta@foxmail.com>",
    "build": {
        "proj_dir": "../depsland",
        "dist_dir": "../dist/depsland-v{app_version}-win-standard",
        "launcher_name": "setup",
        "icon": "",
        "target": [{
            "file": "../depsland/setup/main.py",
            "function": "main",
            "args": ["python39"],
            "kwargs": {}
        }],
        "readme": "",
        "attachments": {
            "../build/assets/windows": "assets",
            "../build/depsland.bat": "asset",
            "../conf": "assets",
            "../pypi/cache": "only_folder",
            "../pypi/downloads": "only_folder",
            "../pypi/extracted": "only_folder",
            "../pypi/index": "only_folder",
            "../requirements.txt": "asset,dist:build/{name}",
            "../sidework": "assets,compile",
            "../venv_home/inventory": "only_folder",
            "../venv_home/venv_links": "only_folder"
        },
        "attachments_exist_scheme": "error",
        "module_paths": [],
        "venv": {
            "enable_venv": true,
            "python_version": "3.9",
            "mode": "embed_python",
            "options": {
                "depsland": {
                    "venv_name": "{app_name_lower}_venv",
                    "venv_id": "",
                    "requirements": "../requirements.txt",
                    "offline": false,
                    "local": ""
                },
                "source_venv": {
                    "path": "../venv1"
                },
                "pip": {
                    "requirements": [],
                    "pypi_url": "https://pypi.python.org/simple/",
                    "local": "",
                    "offline": false
                },
                "embed_python": {
                    "path": "../.venv/python38/embed_venv"
                }
            }
        },
        "compiler": {
            "name": "pyportable_crypto",
            "options": {
                "pyarmor": {
                    "liscense": "trial",
                    "obfuscate_level": 0
                },
                "pyc": {
                    "optimize_level": 0
                },
                "pyportable_crypto": {
                    "key": "depsland_project"
                },
                "zipapp": {
                    "password": ""
                }
            }
        },
        "enable_console": true
    },
    "note": [
        "notice: depsland cannot package itself based on `venv:mode:depsland`",
        "FIXME: you need to manually copy '~/dist/lib/*' to '~/dist/venv/lib/site-packages/*'"
    ],
    "pyportable_installer_version": "4.0.0a1"
}
