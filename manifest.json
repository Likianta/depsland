{
    "appid": "depsland_appstore",
    "name": "Depsland Store",
    "version": "0.6.0b9",
    "assets": {
        "apps/.bin": "root",
        "apps/.venv": "root",
        "build/icon/launcher.ico": "all",
        "config/depsland.yaml": "all",
        "deps": "root",
        "depsland": "all",
        "dist": "root",
        "oss/apps": "root",
        "oss/test": "root",
        "pypi/cache": "root",
        "pypi/downloads": "root",
        "pypi/index": "root",
        "pypi/installed": "root",
        "temp/.self_upgrade": "root",
        "temp/.unittests": "root",
        "CHANGELOG.zh.md": "all",
        "manifest.json": "all",
        "README.md": "all",
        "README.zh.md": "all",
        "poetry.lock": "all"
    },
    "dependencies": {
        "custom_host": [
            "argsense",
            "conflore",
            "lk-logger",
            "lk-utils",
            "qmlease"
        ],
        "official_host": [
            "oss2",
            "poetry",
            "pyyaml",
            "rich",
            "semver",
            "typing_extensions"
        ]
    },
    "launcher": {
        "target": "depsland",
        "type": "package",
        "icon": "build/icon/launcher.ico",
        "args": [
            "launch-gui"
        ],
        "kwargs": {},
        "enable_cli": true,
        "add_to_desktop": true,
        "add_to_start_menu": false,
        "show_console": true
    },
    "depsland_version": "0.6.0b9"
}
